;; seed: 42
(define (problem ztravel-1-2) (:domain zeno-travel)
(:objects
plane1  - aircraft
fl2 fl0 fl1 fl3 fl4 fl5 fl6 - flevel
person1 person2 - person
city0 city1 city2 - city
)

(:init
	(is-true)
	(next fl2 fl3)
	(at plane1 city0)
	(next fl1 fl2)
	(at person1 city0)
	(next fl4 fl5)
	(next fl5 fl6)
	(next fl3 fl4)
	(at person2 city2)
	(fuel-level plane1 fl1)
	(next fl0 fl1)
	(= (refuel-cost plane1 city2) 2026.8124)
	(= (fly-cost plane1 city0 city2) 946.452)
	(= (fly-cost plane1 city1 city2) 16571.2563)
	(= (zoom-cost plane1 city1 city1) 1318.7552)
	(= (zoom-cost plane1 city0 city1) 1179.26)
